<?xml version="1.0" encoding="utf-8" ?>
<users>
    {% for user in users %}
        <user>
            <firstname>
                {{ user.firstname }}
            </firstname>
            <lastname>
                {{ user.lastname }}
            </lastname>
            <email>
                {{ user.email }}
            </email>
            <username>
                {{ user.username }}
            </username>
            <website>
                {{ user.website }}
            </website>
        </user>
    {% endfor %}
</users>
